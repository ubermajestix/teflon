
<% for show in @shows %>
<b><%= show.title %> </b><br/>
show at: <%= nice_time show.showtime %><br/>
$<%= dollar_format show.ticket_price %> <%= show.venue %>, <%= show.city %>, <%= show.state %> <br/>
<%= show.body_html %><br/>

last updated by: <%= show.user.login %> on <%= nice_time show.updated_at %>
<br/>
 <%= link_to 'Edit', edit_show_path(show) %>
 <%= link_to 'Destroy', show, :confirm => 'Are you sure?', :method => :delete %><hr/>
<% end %>
